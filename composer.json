{
	"name": "adam-lutka/php-wndb",
	"description": "PHP API for WordNet (https://wordnet.princeton.edu/)",
	"type": "library",
	"keywords": ["wordnet", "synset"],
	"license": "MIT",
	"minimum-stability": "stable",
	"authors": [
		{
			"name": "Adam Lutka",
			"email": "adam.lutka@gmail.com",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"AL\\PhpWndb\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AL\\PhpWndb\\Tests\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^7.1",
		"phpstan/phpstan": "^0.9.2"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/AdamLutka/PhpEnum"
		}
	],
	"require": {
		"AdamLutka/PhpEnum": "^0.1",
		"php-di/php-di": "^6.0"
	},
	"scripts": {
		"phpstan": "vendor/bin/phpstan analyse src --level=3",
		"tests": "vendor/bin/phpunit"
	}
}
