{
	"name": "adam-lutka/php-wndb",
	"description": "PHP API for WordNet (https://wordnet.princeton.edu/)",
	"type": "library",
	"keywords": ["wordnet", "synset"],
	"license": "MIT",
	"minimum-stability": "stable",
	"authors": [
		{
			"name": "Adam Lutka",
			"email": "adam.lutka@gmail.com",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"AL\\PhpWndb\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AL\\PhpWndb\\Examples\\": "examples/",
			"AL\\PhpWndb\\Tests\\": "tests/",
			"AL\\PhpWndb\\Utils\\": "utils/"
		}
	},
	"require": {
		"php": ">=8.1",
		"symfony/dependency-injection": "^6.2",
		"symfony/config": "^6.2",
		"symfony/yaml": "^6.2",
		"symfony/finder": "^6.2"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.9",
		"phpunit/phpunit": "^10.0"
	},
	"scripts": {
		"phpstan": "vendor/bin/phpstan analyse --level=max src/ tests/ examples/",
		"tests": "vendor/bin/phpunit"
	}
}
